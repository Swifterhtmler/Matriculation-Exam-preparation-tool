{
  "name": "yo-studysuite",
  "productName": "Yo StudySuite",
  "description": "Tool made for preparing to matriculation exams",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": ".vite/build/main.js",
  "scripts": {
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "start": "electron-forge start",
    "package": "vite build && electron-forge package",
    "make": "vite build && electron-forge make",
    "make:win": "vite build && electron-forge make --platform win32",
    "make:mac": "vite build && electron-forge make --platform darwin",
    "make:linux": "vite build && electron-forge make --platform linux",
    "make:all": "vite build && electron-forge make --platform win32,darwin,linux",
    "publish": "electron-forge publish"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.2",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22",
    "@types/quill": "^2.0.14",
    "electron": "^36.7.3",
    "electron-builder": "^26.0.12",
    "jsdom": "^26.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.6",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "easymde": "^2.20.0",
    "electron-squirrel-startup": "^1.0.1",
    "layerchart": "^1.0.11",
    "openai": "^5.10.1",
    "quill": "^2.0.3",
    "svelte-math": "^0.0.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "electron",
      "electron-winstaller"
    ],
    "patchedDependencies": {
      "@sveltejs/kit": "patches/@sveltejs__kit.patch"
    }
  }
}
